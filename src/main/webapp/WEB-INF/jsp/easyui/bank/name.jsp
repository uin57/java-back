<%@page contentType="text/html" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><%@ taglib prefix="sjc" uri="http://www.sjc168.com"%><html xmlns="http://www.w3.org/1999/xhtml"><head><script type="text/javascript">	$(function() {		var options = {			columns : [ [ {				field : 'code',				title : '银行代码'			}, {				field : 'title',				title : '银行名称'			}, {                field : 'bnType',                title : '类型',                formatter : function(value, row) {                    if (value == 1) {                        return '银行';                    } else if (value == 2) {                        var	txt ="";                        if(value!=null){                            txt= "<span  style=\"color:red;\" >非银行</span>";                        }                        return txt;                    } else {                        return value;                    }                }            }, {				field : 'minAmount',				title : '最小额度'			}, {				field : 'maxAmount',				title : '最大额度'			}, {				field : 'depositStatus',				title : '提现',				formatter:function(value,row){					if(value == 0)						return '正常';					else						return '<span style="color:red">禁用</span>';				}			}, {				field : 'rechargeStatus',				title : '充值',				formatter:function(value,row){					if(value == 0)						return '正常';					else						return '<span style="color:red">禁用</span>';				}			}, {				field : 'link',				title : '链接'			}, {				field : 'orderId',				title : '排序'			},{				field : 'other',				title : '操作',				formatter : function(value, row) {					var win ="'#win'";					var url = "'edit'";					var dat = "'edit?id=" + row.id +"'";					var txt = '<sjc:auth url="/admin/bankName/edit"><a href="#" onClick="showWin('+ win +',' +url+ ','+ dat +')">编辑</a></sjc:auth>&nbsp;&nbsp;';					txt += '<sjc:auth url="/admin/bankName/delete"><a href="#" onClick="del(\'delete?id=' + row.id +'\',function(rel){reloadGrid(\'#grid\')})">删除</a></sjc:auth>&nbsp;';					return txt;				}			}] ]		};		createGrid('#grid',options);		createWin('#win');	});</script></head><body>	<div id="grid"></div>	<div id="tb">		<div>			<sjc:auth url="/admin/bankName/add"><a href="#" plain="true" icon="icon-add" class="easyui-linkbutton" onClick="showWin('#win','add')">添加</a></sjc:auth>			<sjc:auth url="/admin/bankName/delete"><a href="#" plain="true" icon="icon-remove" class="easyui-linkbutton" onClick="delAll('delete','#grid')">删除</a></sjc:auth>		</div>	</div>	<div id="win">		<form method="post" style="margin: 20px;">			<input type="hidden" name="id" />			<table class="formtable">				<tr>					<td class="input-title">名称</td>					<td><input type="text" name="title" /></td>				</tr>				<tr>					<td class="input-title">代码</td>					<td><input type="text" name="code" /></td>				</tr>				<tr>					<td class="input-title">类型</td>					<td><select name="bnType">						<option value="1">银行</option>						<option value="2">非银行</option>					</select></td>				</tr>				<tr>					<td class="input-title">排序</td>					<td><input type="text" name="orderId" /></td>				</tr>				<tr>					<td class="input-title">最小额度</td>					<td><input type="text" name="minAmount" /></td>				</tr>				<tr>					<td class="input-title">最大额度</td>					<td><input type="text" name="maxAmount" /></td>				</tr>				<tr>					<td class="input-title">提现状态</td>					<td><select name="depositStatus">						<option value="0">正常</option>						<option value="1">禁用</option>					</select></td>				</tr>				<tr>					<td class="input-title">充值状态</td>					<td><select name="rechargeStatus">						<option value="0">正常</option>						<option value="1">禁用</option>					</select></td>				</tr>				<tr>					<td class="input-title">链接</td>					<td><input type="text" name="link" /></td>				</tr>			</table>			<div style="text-align: center; padding: 5px;">				<a href="#" class="btn-save" icon="icon-save" onClick="saveData(this, function(rel){ closeWin('#win','#grid')})">保存</a>				<a href="#" class="btn-cancel" icon="icon-cancel" onClick="closeWin('#win')">取消</a>			</div>		</form>	</div></body></html>