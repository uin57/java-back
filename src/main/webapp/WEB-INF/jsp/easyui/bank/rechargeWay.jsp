<%@page contentType="text/html" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><%@ taglib prefix="sjc" uri="http://www.sjc168.com"%><html xmlns="http://www.w3.org/1999/xhtml"><head>    <script type="text/javascript">        $(function() {            var options = {                columns : [ [ {                    field : 'alino',                    title : '支付渠道代码'                }, {                    field : 'alias',                    title : '支付渠道名称'                }, {                    field : 'opentype',                    title : '是否打开',                    formatter : function(value, row) {                        if (value == 0) {                            var	txt ="";                            if(value!=null){                                txt= "<span  style=\"color:blue;\" >打开</span>";                            }                            return txt;                        } else if (value == 1) {                            return '关闭';                        } else {                            return value;                        }                    }                }, {                    field : 'waytype',                    title : '渠道类型',                    formatter : function(value, row) {                        if (value == 0) {                            return '其他';                        } else if (value == 1) {                            var	txt ="";                            if(value!=null){                                txt= "<span  style=\"color:red;\" >扫码</span>";                            }                            return txt;                        } else if (value == 2) {                            return '看卡';                        } else if (value == 3) {                            return '线上';                        } else {                            return value;                        }                    }                }, {                    field : 'bnTitle',                    title : '银行对照'                }, {                    field : 'minmoney',                    title : '最小额度'                }, {                    field : 'maxmoney',                    title : '最大额度'                }, {                    field : 'attempty',                    title : '属性能否为空',                    formatter : function(value, row) {                        if (value == 1) {                            var	txt ="";                            if(value!=null){                                txt= "<span  style=\"color:red;\" >不能为空</span>";                            }                            return txt;                        } else if (value == 0) {                            return '可以为空';                        } else {                            return value;                        }                    }                }, {                    field : 'attfirst',                    title : '属性1'                }, {                    field : 'attsecond',                    title : '属性2'                }, {                    field : 'attthird',                    title : '属性3'                }, {                    field : 'ordernum',                    title : '次数'                }, {                    field : 'content',                    title : '说明'                },{                    field : 'other',                    title : '操作',                    formatter : function(value, row) {                        var win ="'#win'";                        var url = "'edit'";                        var dat = "'edit?id=" + row.id +"'";                        var txt = '<sjc:auth url="/admin/rechargeWay/edit"><a href="#" onClick="showWin('+ win +',' +url+ ','+ dat +')">编辑</a></sjc:auth>&nbsp;&nbsp;';                        return txt;                    }                }] ]            };            createGrid('#grid',options);            createWin('#win');        });    </script></head><body><div id="grid"></div><div id="tb">    <div>        <sjc:auth url="/admin/rechargeWay/add"><a href="#" plain="true" icon="icon-add" class="easyui-linkbutton" onClick="showWin('#win','add')">添加</a></sjc:auth>    </div></div><div id="win">    <form method="post" style="margin: 20px;" method="post" enctype="multipart/form-data">        <input type="hidden" name="id" />        <input type="hidden" name="qrcodeurl" />        <input type="hidden" name="imgData" />        <input type="hidden" name="randomNow" />        <input type="hidden" name="randomOld" />        <table class="formtable">            <tr>                <td class="input-title">支付渠道代码</td>                <td><input type="text" name="alino" /></td>            </tr>            <tr>                <td class="input-title">支付渠道名称</td>                <td><input type="text" name="alias" /></td>            </tr>            <tr>                <td class="input-title">最小额度</td>                <td><input type="text" name="minmoney" /></td>            </tr>            <tr>                <td class="input-title">最大额度</td>                <td><input type="text" name="maxmoney" /></td>            </tr>            <tr>                <td class="input-title">是否打开</td>                <td><select name="opentype">                    <option value="0">打开</option>                    <option value="1">关闭</option>                </select></td>            </tr>            <tr>                <td class="input-title">渠道类型</td>                <td><select name="waytype">                    <option value="1">扫码</option>                    <option value="2">看卡</option>                    <option value="3">线上</option>                    <option value="0">其他</option>                </select></td>            </tr>            <tr>                <td class="input-title">银行对照</td>                <td>                    <select name="bnId" >                        <c:forEach items="${bankNameList}" var="bn">                            <option value="${bn.id}">${bn.title}</option>                        </c:forEach>                        <option value="0">其他</option>                    </select>                    &nbsp;<span style="color: #f01717;font-size: 12px">[渠道类型]为“扫码”时选择！</span>                </td>            </tr>            <tr>                <td class="input-title">二维码上传</td>                <td>                    <input id="sourceFile" type="file" name="sourceFile" multiple="true">                </td>            </tr>            <tr>                <td class="input-title">属性能否为空</td>                <td><select name="attempty">                    <option value="0">可以为空</option>                    <option value="1">不能为空</option>                </select></td>            </tr>            <tr>                <td class="input-title">属性1</td>                <td><input type="text" size="50" name="attfirst" /></td>            </tr>            <tr>                <td class="input-title">属性2</td>                <td><input type="text" size="50" name="attsecond" /></td>            </tr>            <tr>                <td class="input-title">属性3</td>                <td><input type="text" size="50" name="attthird" /></td>            </tr>            <tr>                <td class="input-title">说明</td>                <td><input type="text" size="50" name="content" /></td>            </tr>        </table>        <div style="text-align: center; padding: 5px;">            <a href="#" class="btn-save" icon="icon-save" onClick="saveData(this, function(rel){ closeWin('#win','#grid')})">保存</a>            <a href="#" class="btn-cancel" icon="icon-cancel" onClick="closeWin('#win')">取消</a>        </div>    </form></div></body></html>