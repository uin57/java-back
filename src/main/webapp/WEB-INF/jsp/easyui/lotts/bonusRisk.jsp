<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><%@ taglib prefix="sjc" uri="http://www.sjc168.com" %><html xmlns="http://www.w3.org/1999/xhtml"><head>    <script type="text/javascript">        $(function () {            var options = {                singleSelect: true,                columns: [[{                    field: 'seasonId',                    title: '期号'                }, {                    field: 'initNum',                    title: '初始号码'                }, {                    field: 'endNum',                    title: '补开号码'                }, {                    field: 'createNumCount',                    title: '奖号获取次数'                }, {                    field: 'validBetAmount',                    title: '有效投注额'                }, {                    field: 'rebate',                    title: '当期返点'                }, {                    field: 'initBonus',                    title: '初始中奖金额'                }, {                    field: 'initGains',                    title: '初始损益'                }, {                    field: 'endBonus',                    title: '补开后中奖金额'                }, {                    field: 'endGains',                    title: '补开后损益'                }, {                    field: 'bonusPoolLeft',                    title: '奖池剩余金额'                },{                    field: 'secondBonusPoolLeft',                    title: '第二奖池剩余金额'                }, {                    field: 'createTime',                    title: '操作时间'                }]]            };            createGrid('#grid',options);            $("#search").click(function(){                var p = getFormData('form');                console.log(p);                reloadGrid("#grid", p);            });        });        function ajaxData(url, func) {            $.ajax({                url: url,                type: 'GET',                dataType: 'json',                timeout: 500000,                error: function (XMLHttpRequest, textStatus, errorThrown) {                    $.messager.alert('错误', XMLHttpRequest.status + ' ' + XMLHttpRequest.statusText, 'error');                },                success: function (rel) {                    if (rel.status == 200) {                        if (func) {                            func(rel);                        }                    } else {                        $.messager.alert('错误', rel.content, 'error');                    }                }            });        }        function resetNo() {            jQuery('.addSeason').each(function (i, value) {                $(this).attr("name", "simpleSeasons[" + i + "].season");            });            jQuery('.addNums').each(function (i, value) {                $(this).attr("name", "simpleSeasons[" + i + "].nums");            });        }    </script></head><body><div id="grid"></div><div id="tb">    <form id="form" method="get">        <div>            彩种：<select name="lotteryId">                    <option value="">全部</option>                    <option value="dfk3">大順快3</option>                    <option value="sj1fc">大順时时彩</option>                    <option value="ffpk10">大順pk10</option>                </select>            操作时间：<input type="text" id="beginTime" name="beginTime" class="easyui-datetimebox" /> ~ <input type="text" id="endTime" name="endTime" class="easyui-datetimebox"/>            期号：<input type="text" size="20" name="seasonId"/>            是否换号：<select name="isChangeNum">                        <option value="1">换号期数</option>                        <option value="1">全部期数</option>                    </select>            <a href="#" plain="true" id="search" class="easyui-linkbutton" icon="icon-search">查询</a>        </div>    </form></div></body></html>