<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><%@ taglib prefix="sjc" uri="http://www.sjc168.com" %><html xmlns="http://www.w3.org/1999/xhtml"><head>    <script type="text/javascript">        $(function () {            var options = {                singleSelect: true,                columns: [[{                    field: 'lotteryName',                    title: '彩票名称'                }, {                    field: 'dayFlowWater',                    title: '日均流水'                }, {                    field: 'initBonusPool',                    title: '初始奖金池'                }, {                    field: 'revenueRate',                    title: '毛利率'                },  {                    field: 'other',                    title: '操作',                    formatter: function (value, row) {                        var win = "'#win'";                        var url = "'edit'";                        var dat = "'edit?id=" + row.id + "'";                        var txt = '&nbsp;&nbsp;<sjc:auth url="/admin/risk/bonusConfig/edit"><a href="#" onClick="showWin(' + win + ',' + url + ',' + dat + ')">编辑</a></sjc:auth>&nbsp;&nbsp;';                        txt += '&nbsp;&nbsp;<sjc:auth url="/admin/risk/bonusConfig/cleanBonus"><a href="#" onClick="showWin(\'#cleanBonus\',\'cleanBonus\',\'cleanBonus?lotteryId='+row.lotteryId+'\')">手动清除</a></sjc:auth>&nbsp;&nbsp;';                        return txt;                    }                }]]            };            createGrid('#grid', options);            createWin('#win');            createWin('#cleanBonus');            $("#search").click(function () {                var p = getFormData('form');                console.log(p);                reloadGrid("#grid", p);            });        });    </script></head><body><div id="grid"></div><div id="tb">    <div>        <sjc:auth url="/admin/risk/bonusConfig/add"><a href="#" plain="true" icon="icon-add" class="easyui-linkbutton"                                           onClick="showWin('#win','add',{role:0,status:0})">添加</a></sjc:auth>        <sjc:auth url="/admin/risk/bonusConfig/delete"><a href="#" plain="true" icon="icon-remove" class="easyui-linkbutton"                                              onClick="delAll('delete','#grid')">删除</a></sjc:auth>    </div></div><div id="win">    <form method="post" style="margin: 20px;" >        <input name="id" type="hidden" />        <table class="formtable">            <tr>                <td class="input-title">彩种:</td>                <td><select name="lotteryId">                    <option value="">全部</option>                    <option value="dfk3">大順快3</option>                    <option value="sj1fc">大順时时彩</option>                    <option value="ffpk10">大順pk10</option>                </select></td>            </tr>            <tr>                <td class="input-title">日均流水:</td>                <td><input type="text" name="dayFlowWater"/>￥</td>            </tr>            <tr>                <td class="input-title">初始奖金池:</td>                <td><input type="text" name="initBonusPool"/>￥</td>            </tr>            <tr>                <td class="input-title">毛利率:</td>                <td><input  type="text"  name="revenueRate"/>%</td>            </tr>        </table>        <div style="text-align: center; padding: 5px;">            <a href="#" class="btn-save" icon="icon-save"               onClick="saveData(this, function(rel){ closeWin('#win','#grid')})">保存</a>            <a href="#" class="btn-cancel" icon="icon-cancel" onClick="closeWin('#win')">取消</a>        </div>    </form></div><div id="cleanBonus">    <form method="post" style="margin: 20px;" >        <input name="lotteryId" type="hidden" />        <table class="formtable">            <tr>                <td class="input-title">清除金额:</td>                <td><input type="text" name="cleanAmount"/>￥</td>            </tr>        </table>        <div style="text-align: center; padding: 5px;">            <a href="#" class="btn-save" icon="icon-save"               onClick="saveData(this, function(rel){ closeWin('#cleanBonus','#grid')})">保存</a>            <a href="#" class="btn-cancel" icon="icon-cancel" onClick="closeWin('#cleanBonus')">取消</a>        </div>    </form></div></body></html>